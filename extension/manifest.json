{
    "name":"Resource Finder",
    "description":"We provide you with the details of all the resources provided by New Jersey's economy.",
    "version":"1.0",

    "manifest_version": 2,

    "browser_action": {
        "default_icon":"icon.png",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["background.js"]
    },

    "permissions": [
        "tabs",
        "bookmarks",
        "downloads",
        "webRequest",
        "webRequestBlocking"
    ]
}